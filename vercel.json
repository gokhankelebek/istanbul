{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/((?!.*\\.).*)", "destination": "/$1/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
